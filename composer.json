{
    "name": "anisaronno/laravel-media-helper",
    "description": "Laravel Media Helper",
    "version": "0.4.1",
    "license": "MIT",
    "keywords": [
        "laravel-media-helper",
        "WordPress",
        "Laravel",
        "wedevs"
    ],
    "authors": [
        {
            "name": "MD. Anichur Rahaman",
            "email": "contact@anichur.com",
            "homepage": "https://anichur.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/anisAronno/laravel-media-helper",
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.0",
        "pestphp/pest": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "AnisAronno\\MediaHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnisAronno\\MediaHelper\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AnisAronno\\LaravelSettings\\MediaHelperServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
